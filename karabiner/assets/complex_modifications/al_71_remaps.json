{
  "title": "Remap Nav Cluster (Insert/Home/PgUp/Del/End/PgDn)",
  "rules": [
    {
      "description": "Remap 6-key Nav Cluster to Insert/Del, PgUp/PgDn, Home/End layout",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "insert" },
          "to": [{ "key_code": "insert" }]
        },
        {
          "type": "basic",
          "from": { "key_code": "home" },
          "to": [{ "key_code": "delete_forward" }]
        },
        {
          "type": "basic",
          "from": { "key_code": "end" },
          "to": [{ "key_code": "page_up" }]
        },
        {
          "type": "basic",
          "from": { "key_code": "page_up" },
          "to": [{ "key_code": "page_down" }]
        },
        {
          "type": "basic",
          "from": { "key_code": "delete_forward" },
          "to": [{ "key_code": "home" }]
        },
        {
          "type": "basic",
          "from": { "key_code": "page_down" },
          "to": [{ "key_code": "end" }]
        }
      ]
    }
  ]
}
